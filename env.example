# Backend Environment Variables
NODE_ENV=development
PORT=3000
DB_HOST=postgres
DB_PORT=5432
DB_NAME=livestream_db
DB_USER=postgres
DB_PASSWORD=password
JWT_SECRET=your_jwt_secret_key_change_in_production

# Frontend Environment Variables
REACT_APP_API_URL=http://localhost:3000
REACT_APP_SRS_URL=http://localhost:8000

# SRS Configuration
SRS_LOG_LEVEL=info
SRS_HTTP_API_ENABLED=true
SRS_HTTP_CALLBACK_ENABLED=true

# CDN Configuration (for production)
CDN_DOMAIN=your-cdn-domain.com
CDN_RTMP_URL=rtmp://your-cdn-domain.com/live
CDN_HLS_URL=https://your-cdn-domain.com/live
